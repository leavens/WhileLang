% original program
proc exprs(val a,b,c,q,r,y res x,z) is {
    x := 1+2-3*q--r;  % 1
    a := b/(b-c);     % 2
    if (x > q and a < b) % 3 
    then
        z := (2*(a*b)+c*c)-4*a*c % 4
    else {
        while (x < 0 or -y > 3 and b <= c and c >= b) % 5
        do z := x*x*x*(-q) % 6
    }
}